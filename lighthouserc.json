{
  "ci": {
    "collect": {
      "staticDistDir": "./build",
      "url": ["http://localhost:3000"],
      "numberOfRuns": 2
    },
    "assert": {
      "assertions": {
        "categories:accessibility": ["error", { "minScore": 0.9 }]
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./lhci_reports"
    }
  }
}
